<h1>Ajouter une liste</h1>
<%= simple_form_for(@list) do |f| %>
  <div class="form-inputs">
  <%= f.input :name %>
  <%= f.input :photo, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
<p><%= link_to "Retour aux listes", lists_path, class:'text-decoration-none' %></p>
